    },
    {
      "reg": "^\\/node_modules\\/vue-select\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/vue-select\\/(select\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/vue-select\\/(select\\.html)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
