{
  "build": "rm package.json && npm install --prefix . countup.js@1.7.1",
  "name": "countup.js",
  "description": "Animates a numerical value by counting to it",
  "repos": "https://github.com/inorganik/countUp.js.git",
  "main": "countUp.min.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/countup\\.js\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/countup\\.js\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "paths": {
    "dist": "."
  },
  "tags": [
    {
      "version": "1.7.1",
      "__hash": "501b62db8c48c0ee8bee9a816b1e74b139ce83b0"
    }
  ]
}